<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane minHeight="200.0" minWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Id predmeta" />
        <Label text="Naziv predmeta" GridPane.rowIndex="1" />
        <Label text="Profesor" GridPane.rowIndex="2" />
        <TextField fx:id="fieldId" disable="true" editable="false" GridPane.columnIndex="1" />
        <TextField fx:id="fieldName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <ChoiceBox fx:id="choiceProfessor" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <buttons>
                <Button fx:id="btnOk" defaultButton="true" mnemonicParsing="false" text="OK" />
                <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" text="Izlaz" />
            </buttons>
        </ButtonBar>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</GridPane>
